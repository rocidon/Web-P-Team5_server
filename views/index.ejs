<!DOCTYPE html>
<html lang="ko">
  <head>
    <meta charse="UTF-8" />
    <title>User list</title>
  </head>
  <body>
    <table>
      <tr>
        <th>이름</th>
        <th>이메일</th>
        <th>수정</th>
        <th>삭제</th>
      </tr>
      <% users.forEach(function (value) { %>
      <tr>
        <td><%= value.name %></td>
        <td><%= value.email %></td>
        <td><a href="/edit/<%= value.id %>">수정</a></td>
        <td><a href="/delete/<%= value.id %>">삭제</a></td>
      </tr>
      <% }); %>
    </table>
  </body>
</html>
